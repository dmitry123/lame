static class Lame {

	private Lame() {
		/* Locked */
	}

	interface MainListener {
		void onStart();
		void onFinish();
	}

	abstract class MainAdapter implements MainListener {
		void onStart() {}
		void onFinish() {}
	}
}

class Main extends Lame.MainAdapter {
	public void onStart() {}
	public void onFinish() {}
}

Lame lame;